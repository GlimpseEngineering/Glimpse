import React, { Component } from 'react';


class Posts extends Component {



  render() {

    return (
      <div className="feed_content">
        <ul>
          {this.props.userPosts.map( post =>
            <li key={post.id} className='list-item'>{post.description}</li>
          )}
        </ul>
      </div>
    );
  }
}

export default Posts;
